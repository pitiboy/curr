{
  "kind": "collectionType",
  "collectionName": "members",
  "info": {
    "singularName": "member",
    "pluralName": "members",
    "displayName": "Member",
    "description": "A community participant (individual)"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "identifier": {
      "type": "uid",
      "targetField": "name",
      "required": true
    },
    "name": {
      "type": "string",
      "maxLength": 255,
      "required": true
    },
    "email": {
      "type": "email",
      "required": true,
      "unique": true
    },
    "memberships": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::membership.membership",
      "mappedBy": "member"
    },
    "accounts": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::account.account"
    }
  }
}
