{
  "kind": "collectionType",
  "collectionName": "members",
  "info": {
    "singularName": "member",
    "pluralName": "members",
    "displayName": "Member",
    "description": "A community participant (individual)"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "identifier": {
      "type": "uid",
      "required": true,
      "unique": true
    },
    "name": {
      "type": "string",
      "required": true,
      "maxLength": 255
    },
    "email": {
      "type": "email",
      "required": true,
      "unique": true
    },
    "status": {
      "type": "enumeration",
      "enum": ["resident", "active", "supporting", "sympathizer"],
      "default": "active",
      "required": true
    },
    "memberships": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::membership.membership",
      "mappedBy": "member"
    },
    "transactions": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::transaction.transaction",
      "mappedBy": "member"
    }
  }
}
