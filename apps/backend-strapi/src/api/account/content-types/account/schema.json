{
  "kind": "collectionType",
  "collectionName": "accounts",
  "info": {
    "singularName": "account",
    "pluralName": "accounts",
    "displayName": "Account",
    "description": "Financial account within a division for tracking transactions"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true,
      "maxLength": 255
    },
    "code": {
      "type": "string",
      "maxLength": 10
    },
    "category": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::account-category.account-category",
      "inversedBy": "accounts"
    },
    "description": {
      "type": "text"
    },
    "organization": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::organization.organization",
      "inversedBy": "accounts"
    },
    "parent": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::account.account",
      "inversedBy": "children"
    },
    "children": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::account.account",
      "mappedBy": "parent"
    },
    "debit_transactions": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::transaction.transaction",
      "mappedBy": "debit_account"
    },
    "credit_transactions": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::transaction.transaction",
      "mappedBy": "credit_account"
    }
  }
}
