{
  "kind": "collectionType",
  "collectionName": "membership_types",
  "info": {
    "singularName": "membership-type",
    "pluralName": "membership-types",
    "displayName": "Membership Type",
    "description": "Types of membership roles within organizations (tag, bentlakó tag, szimpatizáns, etc.)"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "code": {
      "type": "string",
      "maxLength": 50,
      "required": true,
      "unique": true
    },
    "name": {
      "type": "string",
      "maxLength": 255,
      "required": true
    },
    "description": {
      "type": "text"
    },
    "color": {
      "type": "string",
      "maxLength": 7
    },
    "memberships": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::membership.membership",
      "mappedBy": "membership_type"
    }
  }
}
